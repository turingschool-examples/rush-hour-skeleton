
<nav><%= partial 'partials/navigation' %></nav>

	<h2>Application Event Index</h2>

	<h4>Received Events:</h4>
	<h3>(sorted most to least)</h3>

	<ol id="events">
		<%@events.each do |(event, count)| %>
		<li> <a href="/sources/<%= @source.identifier%>/events/<%=event.name%>"><%=event.name%></a></li>
		<% end %>

	</ol>

