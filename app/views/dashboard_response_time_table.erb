<table id="response-times" class="table table-striped table-bordered table-hover table-sm">
  <tr class="info">
    <td> Average Response Time </td>
    <td> <%= average %> milliseconds</td>
  </tr>
  <tr class="active">
    <td> Minimum Response Time </td>
    <td> <%= min %> milliseconds </td>
  </tr>
  <tr class="info">
    <td> Maximum Response Time </td>
    <td> <%= max %> milliseconds </td>
  </tr>
  <% if freq %>
    <tr class="active">
      <td> Most Frequent Request Type </td>
      <td> <%= freq  %> </td>
    </tr>
  <% end %>
</table>
